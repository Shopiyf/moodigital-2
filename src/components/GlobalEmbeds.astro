}  
.dropdown-item[data-state='open']  .dropdown-bg  {  
opacity:  1;  
transform:  scale(1);  
}  
/*  Button  Character  Stagger  */  
/*  Characters  */  
.btn_wrap  [data-button-animate-chars],  
a  [data-button-animate-chars]  {  
overflow:  hidden;  
position:  relative;  
display:  inline-block;  
}  
 
.btn_wrap  [data-button-animate-chars]  span,  
a  [data-button-animate-chars]  span  {  
display:  inline-block;  
position:  relative;  
text-shadow:  0px  1.3em  currentColor;  
transform:  translateY(0em)  rotate(0.001deg);  
transition:  transform  0.5s  var(--ease-main);  
}  
 
.btn_wrap:hover  [data-button-animate-chars]  span,  
a:hover  [data-button-animate-chars]  span  {  
transform:  translateY(-1.3em)  rotate(0.001deg);  
}  
/*  Marquee  */  
@keyframes  translateX  {  
0%  {  
transform:  translateX(0);  
}  
100%  {  
transform:  translateX(-100%);  
}  
}  
@keyframes  translateY  {  
0%  {  
transform:  translateY(0);  
}  
100%  {  
transform:  translateY(-100%);  
}  
}  
/*  Horizontal  (par  défaut)  */  
[data-marquee-list]  {  
animation:  translateX  30s  linear  infinite;  
animation-play-state:  paused;  
will-change:  transform;  
}  
/*  Reverse  horizontal  */  
[data-marquee-reverse]  [data-marquee-list]  {  
animation-direction:  reverse;  
}  
/*  Vertical  */  
[data-marquee-direction='vertical']  [data-marquee-list]  {  
animation:  translateY  30s  linear  infinite;  
animation-play-state:  paused;  
}  
/*  Reverse  vertical  */  
[data-marquee-direction='vertical'][data-marquee-reverse]  [data-marquee-list]  {  
animation-direction:  reverse;  
}  
/*  Filters  */  
/*  Content  btn  */  
