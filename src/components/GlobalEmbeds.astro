}  
.dropdown-item[data-state='open']  .dropdown-icon  {  
transform:  rotate(225deg);  
}  
.dropdown-bg  {  
transition:  transform  var(--duration-half)  var(--ease-main),  opacity  var(--duration-half)  var(--ease-main);  
opacity:  0;  
